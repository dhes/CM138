{
  "resourceType": "Questionnaire",
  "id": "populate-observation-example",
  "url": "http://example.org/fhir/Questionnaire/populate-example",
  "title": "Pre-Filled Patient Name & Tobacco Use Status",
  "status": "active",
  "extension": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-launchContext",
      "extension": [
        {
          "url": "name",
          "valueString": "Patient"
        },
        {
          "url": "type",
          "valueCode": "Patient"
        }
      ]
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-launchContext",
      "extension": [
        {
          "url": "name",
          "valueString": "Observation"
        },
        {
          "url": "type",
          "valueCode": "Observation"
        }
      ]
    }
  ],
  "item": [
    {
      "linkId": "patient-name",
      "text": "Patient's Name",
      "type": "string",
      "extension": [
        {
          "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
          "valueExpression": {
            "language": "text/fhirpath",
            "expression": "Patient.name.given.first()"
          }
        }
      ]
    },
    {
      "linkId": "tobacco-use-status",
      "text": "Tobacco Use Status",
      "type": "choice",
      "extension": [
        {
          "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
          "valueExpression": {
            "language": "text/fhirpath",
            "expression": "%questionnaire.Observation.valueCodeableConcept.coding.display.first()"
          }
        }
      ]
    }
  ]
}
